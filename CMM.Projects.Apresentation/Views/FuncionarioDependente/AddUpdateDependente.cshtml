@model CMM.Projects.Apresentation.Models.FuncionarioDependenteModelView

<div class="modal-header">
    <h3 class="modal-title"><span class="fa fa-edit"></span> @ViewBag.Title</h3>

    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>

</div>
<div class="modal-body">
    @using (Html.BeginForm("AddUpdateDependente", "FuncionarioDependente", FormMethod.Post, new { id = "formCrud", @autocomplete="off" }))
    {
        @Html.AntiForgeryToken()

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.FUNDEP_ID)
        @Html.HiddenFor(model => model.FUN_ID)

        <div class="row ">
            <div class="form-group col-md-12">
                @Html.LabelFor(model => model.FUNDEP_NOME, htmlAttributes: new { @class = "control-label" })

                @Html.EditorFor(model => model.FUNDEP_NOME, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                @Html.ValidationMessageFor(model => model.FUNDEP_NOME, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="row ">
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.FUNDEP_TIPO, htmlAttributes: new { @class = "control-label" })

                @Html.DropDownListFor(model => model.FUNDEP_TIPO, (SelectList)ViewBag.FUN_TIPO, "Selecione", htmlAttributes: new { @class = "form-control form-control-sm", @required = true })
                @Html.ValidationMessageFor(model => model.FUNDEP_TIPO, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.FUNDEP_CPF, htmlAttributes: new { @class = "control-label" })

                @Html.EditorFor(model => model.FUNDEP_CPF, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                @Html.ValidationMessageFor(model => model.FUNDEP_CPF, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.FUNDEP_DATANASCIMENTO, htmlAttributes: new { @class = "control-label" })

                @Html.EditorFor(model => model.FUNDEP_DATANASCIMENTO, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                @Html.ValidationMessageFor(model => model.FUNDEP_DATANASCIMENTO, "", new { @class = "text-danger" })
            </div>
        </div>






    }
</div>
<div class="modal-footer">
    <a data-dismiss="modal" class="btn btn-sm btn-outline-secondary">Voltar</a>
    <input type="button" value="Salvar" class="btn btn-secondary" />
</div>


@Scripts.Render("~/bundles/jqueryval")
<script src="~/Scripts/ccm.projects/EnviaFormCad.js"></script>
@model CMM.Projects.Apresentation.Models.FuncionarioDependenteModelView

<div class="modal-header">
    <h4>Você tem certeza que deseja excluir este registro?</h4>
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
</div>
<div class="row">

    <div class="modal-body">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.FUNDEP_CPF)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FUNDEP_CPF)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.FUNDEP_NOME)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FUNDEP_NOME)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.FUNDEP_TIPO)
            </dt>

            <dd>
                @Html.DisplayFor(model => model._TIPODEPENDENTE)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.FUNDEP_DATANASCIMENTO)
            </dt>

            <dd>
                @if (Model.FUNDEP_DATANASCIMENTO.HasValue)
                {
                    @Model.FUNDEP_DATANASCIMENTO.Value.ToString("dd/MM/yyyy")
                }
                @*@Html.DisplayFor(model => model.FUNDEP_DATANASCIMENTO, "{0:dd/MM/yyyy}")*@
            </dd>

        </dl>
    </div>
</div>

@using (Html.BeginForm("Delete", "FuncionarioDependente", FormMethod.Post, new { id = "formCrud" }))
{
    @Html.AntiForgeryToken()

    <div class="modal-footer">
        <a data-dismiss="modal" class="btn btn-sm btn-outline-secondary">Voltar</a>
        <input type="button" value="Excluir" class="btn btn-outline-danger" />
    </div>
}





<script src="~/Scripts/ccm.projects/EnviaFormCad.js"></script>
@Scripts.Render("~/bundles/jqueryval")
